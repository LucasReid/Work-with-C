#include <stdio.h>
#include <stdlib.h>

int main(int argc, char** argv)
{
	int *pointer;
	*pointer = 10;
	
	pointer=(int *) malloc(sizeof ( int) );
	if(pointer = NULL)
	{
		printf("unable to allocate memory\n");
		return 1;
	}
	printf("The address of pointer is %p\n",pointer);
	printf("The contents of pointer is %d\n",*pointer);
	
	return 0;
}
